(function authorsList($){Drupal.behaviors.authorsList={attach(context){const $jsShowAuthor=$('.js-show-author',context);const hiddenFlag='hidden';$jsShowAuthor.on('click',(e)=>{this.showAuthor(e,hiddenFlag);});},showAuthor(el,hiddenFlag){const $this=$(el.target);$this.prev('ul').find('li').removeClass(hiddenFlag);$this.remove();}};})(jQuery);;
